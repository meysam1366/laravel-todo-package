{
    "name": "meysammaghsoudi/todopackage",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "meysammaghsoudi\\Todopackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "meysammaghsoudi\\Todopackage\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Meysam Maghsoudi",
            "email": "safir.1987@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "laravel/framework": "7.*|8.*|9.*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.4",
        "phpunit/phpunit": "^9.5.10"
    },
    "extra": {
        "laravel":{
            "providers": [
                "meysammaghsoudi\\Todopackage\\ToDoServiceProvider"
            ],
            "dont-discover": [
                "meysammaghsoudi/Todopackage"
            ]
        }
    }
}
